{
  "name": "mailpeek",
  "version": "0.0.2",
  "description": "Provides a web interface to view emails sent out when developing in Rails.",
  "scripts": {
    "build": "webpack",
    "start": "webpack --progress --colors --watch",
    "deploy": "NODE_ENV=production webpack --progress -p",
    "stats": "webpack --json > stats.json"
  },
