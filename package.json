  "bugs": {
    "url": "https://github.com/RoguePod/mailpeek/issues"
  },
  "homepage": "https://github.com/RoguePod/mailpeek#readme",
  "dependencies": {
    "font-awesome": "*",
    "jquery": "*",
    "lodash": "*",
    "moment": "*",
    "normalize.css": "*",
    "react": "*",
    "react-dom": "*",
    "validatorjs": "*"
  },
  "devDependencies": {
    "autoprefixer": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "css-loader": "*",
    "cssnano": "*",
    "file-loader": "*",
    "imports-loader": "*",
    "lost": "*",
